	"dependencies": {
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@material-ui/styles": "^4.11.4",
		"@mui/lab": "^5.0.0-alpha.49",
		"@mui/material": "^5.0.1",
		"@reduxjs/toolkit": "^1.5.1",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/jest": "^26.0.23",
		"@types/node": "^15.6.1",
		"@types/react": "^17.0.8",
		"@types/react-dom": "^17.0.5",
		"@types/react-redux": "^7.1.16",
		"@types/react-router-dom": "^5.1.7",
		"axios": "^0.21.1",
		"concurrently": "^6.2.0",
		"cosmiconfig": "^7.0.0",
		"i18next": "^20.3.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-hook-form": "^7.7.1",
		"react-i18next": "^11.10.0",
		"react-query": "^3.16.0",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"react-scripts": "4.0.3",
		"redux": "^4.1.0",
		"resolve": "^1.20.0",
		"typescript": "^4.3.2",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public",
		"cypress-open": "cypress open",
		"cypress-run": "cypress run --spec cypress/integration/*",
		"test-cypress-run": "concurrently \"npm:start-dev\" \"npm:cypress-run\"",
		"test-cypress-open": "concurrently \"npm:start-dev\" \"npm:cypress-open\"",
		"start-dev-server": "cd data && npm run start:dev",
		"start-dev": "concurrently \"npm:start-dev-server\" \"npm:start\"",
		"start-build-server": "cd data && npm install && npm run start",
		"start-build": "concurrently \"npm:start-build-server\" \"npm:start\"",
		"install-server": "cd data && npm install",
		"start-jest": "jest",
		"ci": "start-server-and-test start-build http://localhost:4000 cypress-run",
		"plop": "plop"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@cypress/browserify-preprocessor": "^3.0.1",
		"@types/cypress-cucumber-preprocessor": "^4.0.1",
		"@types/lodash": "^4.14.175",
		"@types/uuid": "^8.3.1",
		"cypress": "^7.4.0",
		"cypress-cucumber-preprocessor": "^4.1.3",
		"eslint-plugin-cypress": "^2.11.3",
		"plop": "^2.7.4",
		"start-server-and-test": "^1.12.5"
	},
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": true
	}
}
